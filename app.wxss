/**app.wxss**/
@import "static/weui.wxss";
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.row {
  display: flex;
  flex-direction: row;
}

.rowCenter {
  display: flex;
  align-items: center;
}

.rowBothEnds {
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.center {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
